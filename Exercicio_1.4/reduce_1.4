#!/usr/bin/python3

import sys

salesTotal = 0
oldKey = None
maior = 0
# Se hace un bucle alrededor de los datos
# Estará en el formato llave\tval
# Donde clave es el nombre de la tienda, val es el importe de la venta
#
# Se presentarán todas las ventas de una tienda en particular,
# luego la clave cambiará y estaremos tratando con la siguiente tienda

for line in sys.stdin:
    data_mapped = line.strip().split("\t")
    if len(data_mapped) != 2:
        # Algo ha ido mal. Omita esta línea.
        continue

    thisKey, thisSale = data_mapped

    # Escribe un par key:value ante un cambio na key
    # Reinicia o total
    if oldKey and oldKey != thisKey:
        print(oldKey, "\t", maior)
        oldKey = thisKey;  # creo que sobra esta liña
        maior = 0

    oldKey = thisKey
    if thisSale >= maior:
        maior = float(thisSale)

# Escribe o último par, unha vez rematado o bucle
if oldKey != None:
    print(oldKey, "\t", maior)